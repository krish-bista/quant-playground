    "import numpy as np\n",
    "\n",
    "# Create an array\n",
    "a = np.array([[1, 2], [3, 4]])\n",
    "print(\"Array:\\n\", a)\n",
    "\n",
    "# Dot product\n",
    "b = np.array([[5, 6], [7, 8]])\n",
    "dot = np.dot(a, b)\n",
    "print(\"Dot Product:\\n\", dot)"
   ]
